# Example environment variables for backend
# Path to Google service account JSON if using Vertex AI client libs
GOOGLE_APPLICATION_CREDENTIALS=
# Vertex / Gemini model id (if using Vertex AI generative models). Example: "projects/PROJECT/locations/LOCATION/models/MODEL"
GEMINI_MODEL_ID=
# Optional: custom endpoint or API key if using a custom Gemini wrapper
GEMINI_API_KEY=
# Uploads path
UPLOADS_DIR=./uploads
# Tesseract executable path (if needed on Windows)
TESSERACT_CMD=
